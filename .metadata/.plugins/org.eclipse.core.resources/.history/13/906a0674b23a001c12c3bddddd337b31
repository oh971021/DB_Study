create table BMI (
	b_no number (2) primary key,
	b_name varchar (10 char) not null,
	b_weight number (10) not null,
	b_height number (10) not null,
	b_bmi number (3, 2) not null,
	b_result varchar (10 char) not null
);

create sequence bmi_seq;

insert into bmi values (99, "JS", 178, 74, 23.24, "Á¤»ó");

drop table bmi cascade constraint purge;